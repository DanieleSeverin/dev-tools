{
"$schema": "https://schema.tauri.app/config/2",
"productName": "DevTools Suite",
"identifier": "com.example.devtools",
"version": "0.1.0",


"build": {
"devUrl": "http://localhost:4200",
"frontendDist": "../dist/dev-tools/browser"
},


"app": {
    "security": { "csp": null },
    "windows": [
        {
            "label": "main",
            "title": "DevTools Suite",
            "width": 1100,
            "height": 720,
            "resizable": true
        }
    ]
},


"bundle": {
"active": true,
"targets": "all",
"windows": {
"webviewInstallMode": { "type": "downloadBootstrapper", "silent": true }
}
},


"plugins": {}
}